<img src="{{ QRCode }}" alt="{{ SecretCode }}">

<pre>{{ SecretCode }}</pre>

<form method="post">
   <label>
       Token
       <input type="text" pattern="[0-9]{6}" name="token" required>
   </label>
    <input type="hidden" name="{{ CSRFFormName }}" value="{{ CSRFToken }}">
    <input type="submit" value="Send">
</form>
